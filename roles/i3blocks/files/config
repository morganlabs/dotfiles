[memory]
command=$SCRIPT_DIR/memory
label=MEM 
interval=2

[brightness]
command=$SCRIPT_DIR/brightness
interval=10
signal=20
label=BRI 

[volume]
command=$SCRIPT_DIR/volume
interval=10
signal=10

[battery]
command=$SCRIPT_DIR/battery
interval=2
label=BAT 
BAT_NUMBER=0

[time]
command=date +'%A %d %B %H:%M:%S'
interval=1
